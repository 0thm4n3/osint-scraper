            <li><span class='bold'>Name:</span>{{ twitter.name }}</li>
        {% endif %}
        {% if twitter.location %}
            <li><span class='bold'>Location:</span>{{ twitter.location }}</li>
        {% endif %}
        {% if twitter.description %}
            <li><span class='bold'>Bio:</span>{{ twitter.description }}</li>
        {% endif %}
        {% if twitter.created_at %}
            <li><span class='bold'>User Since:</span>{{ twitter.created_at }}</li>
        {% endif %}

                </ul>
            </div>
            {% if twitter.avatar %}
                <img class="photo-pic found" src="{{ twitter.avatar }}">
            {% endif %}
            {% if not twitter.avatar %}
                <img class="photo-pic found" src="./../static/twitter_bird.png">
            {% endif %}
        </div>
        <div class="photo-foot card-header"><a target="_blank" href="{{ twitter.url }}" class='photo-a'>Twitter</a>
        </div>
        </div>
