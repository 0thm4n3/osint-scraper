{% extends "layout.jinja2" %}

{% block content %}
<div class="content">
    <h1>Search results</h1>
    <h3>{{ user.login }}</h3>
    <p>{{ user.bio }}</p>
    {% for result in results %}
        <div>
        <a href=""><h2>{{ result.site }}</h2></a>
        <span>{{ result.one }}</span>
        <span>{{ result.two }}</span>
        <span>{{ result.three }}</span>
        <span>{{ result.four }}</span>
        </div>
    {% endfor %}
</div>
{% endblock content %}
