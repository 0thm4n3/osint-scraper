        <div class="col-lg-3 col-md-4 col-xs-6">
        {% if facebook %}
        <div class="photo-div card border-secondary mb-3" style="max-width: 20rem;">
            <div class="card-body text-secondary">
                <div class='site-detail'>Click link to see potential Facebook profile. Make sure you are logged in to Facebook beforehand.</div>
                <img class="photo-pic found" src="../static/facebook.png"/>
            </div>
        <div class="photo-foot card-header"><a target="_blank" href="{{ facebook.url }}" class='photo-a'>Facebook</a>
        </div>
        </div>
        {% endif %}
        {% if not facebook %}
        <div class="photo-div card border-secondary mb-3 not-found" style="max-width: 20rem;">
        <div class="card-body text-secondary">
        <img class='photo-pic' src="./../static/black_x.png" alt="">
        </div>
        <div class="photo-foot card-header"><p class="photo-a strike-out">Facebook</p>
        </div>
        </div>
        {% endif %}
        </div>


